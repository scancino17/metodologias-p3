<Window x:Class="SMSineitor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SMSineitor"
        mc:Ignorable="d"
        Title="MainWindow" Height="540" Width="800">
    <StackPanel>
		<TextBlock>Número</TextBlock>
		<TextBox x:Name="PhoneNumber"></TextBox>
		<TextBlock>Nombre</TextBlock>
		<TextBox x:Name="Name"></TextBox>
		<TextBlock>Compañía</TextBlock>
		<ComboBox x:Name="Companies">
			<ComboBoxItem>Claro</ComboBoxItem>
			<ComboBoxItem>Entel</ComboBoxItem>
			<ComboBoxItem>Grupo GTD</ComboBoxItem>
			<ComboBoxItem>Movistar</ComboBoxItem>
			<ComboBoxItem>Nextel</ComboBoxItem>
			<ComboBoxItem>Simple</ComboBoxItem>
			<ComboBoxItem>Virgin Mobile</ComboBoxItem>
			<ComboBoxItem>VTR</ComboBoxItem>
			<ComboBoxItem>WOM</ComboBoxItem>
		</ComboBox>
		<TextBlock>Mensaje</TextBlock>
		<TextBox x:Name="Message"></TextBox>
		<Button x:Name="Emoji1">:)</Button>
		<Button x:Name="Emoji2">:(</Button>
		<Button x:Name="Emoji3">:|</Button>
		<Button x:Name="Emoji4">:/</Button>
		<Button x:Name="Emoji5">UwU</Button>
		<Button x:Name="Emoji6">xD</Button>
		<Button x:Name="Emoji7">:O</Button>
		<Button x:Name="Emoji8">D:</Button>
		<Button x:Name="Emoji9">:W</Button>
		<Button x:Name="Emoji10">:D</Button>
		<TextBlock x:Name="TimeAndDate">Hora servidor</TextBlock>
		<TextBlock x:Name="CaptchaText">Captcha: 2+2</TextBlock>
		<TextBlock x:Name="Captcha"></TextBlock>
		<Button x:Name="Send">Enviar</Button>
		<TextBlock>Programar envío</TextBlock>
		<TextBox x:Name="TimeAndDateQueue"></TextBox>
		<Button x:Name="Queue">Programar</Button>
		<Image Source="download.png" Height="48"></Image>
	</StackPanel>
</Window>
